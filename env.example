# Fencing XML Processor Environment Variables

# Server Configuration
PORT=3000
NODE_ENV=development

# Logging
LOG_LEVEL=info

# API Configuration
API_PREFIX=api
CORS_ENABLED=true

# Fencing Module Configuration
FENCING_DEFAULT_DISCIPLINE=FEN
FENCING_DEFAULT_VENUE=FEN
FENCING_DEFAULT_LOCATION=FEN

# External API Configuration
API_BASE_URL=http://localhost:3001
API_KEY=your-api-key-here

# MQTT Configuration (Optional - only needed if using microservices)
MQTT_URI=mqtt://localhost:1883
MQTT_USER=your-mqtt-username
MQTT_PASS=your-mqtt-password

